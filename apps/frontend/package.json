{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm exec vite",
    "build": "pnpm exec tsc -b && pnpm exec vite build",
    "lint": "pnpm exec eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "test": "pnpm exec vitest run",
    "test:watch": "pnpm exec vitest",
    "typecheck": "pnpm exec tsc --noEmit",
    "preview": "pnpm exec vite preview"
  },
  "dependencies": {
    "@taskflow/shared": "workspace:^",
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.62.4",
    "@tanstack/react-query-devtools": "^5.62.4",
    "axios": "^1.7.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.11",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^24.1.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^7.1.9",
    "vitest": "^2.1.4"
  }
}
